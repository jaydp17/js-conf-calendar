{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "object-literal-sort-keys": false,
    "no-console": {
      "severity": "warning"
    },
    "interface-name": false,
    "curly": {
      "options": "ignore-same-line"
    }
  },
  "rulesDirectory": []
}
